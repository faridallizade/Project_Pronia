@using Project_Pronia.Models;
@model List<Setting>
<table class="table">
	<thead>
		<tr>
			<th scope="col">Key</th>
			<th scope="col">Value</th>
			<th scope="col"></th>
		</tr>
	</thead>
	<tbody>
		<tr>
			@foreach (var item in Model)
			{
				<td>Key 1</td>
				<td>Value 1</td>
				<td>
					<a asp-controller="setting" asp-action="Update" asp-route-id="@item.Id" class="btn btn-dark">Update</a>
					<a asp-controller="setting" asp-action="Delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a>
				</td>
			}
		</tr>
	</tbody>
</table>

